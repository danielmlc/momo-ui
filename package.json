{
  "name": "momo-ui-monorepo",
  "version": "1.0.0",
  "description": "一个现代化的新拟物化风格组件库 Monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "build:ui": "pnpm --filter @momo-ui/core build",
    "build:demo": "pnpm --filter @momo-ui/demo build",
    "build:docs": "pnpm --filter @momo-ui/docs build",
    "dev:ui": "pnpm --filter @momo-ui/core dev",
    "dev:demo": "pnpm --filter @momo-ui/demo dev",
    "dev:docs": "pnpm --filter @momo-ui/docs dev",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r exec rm -rf dist demo-dist node_modules/.cache",
    "install-all": "pnpm install"
  },
  "keywords": ["neumorphism", "ui", "components", "css", "typescript", "monorepo"],
  "author": "Momo UI Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@types/node": "^20.4.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "packageManager": "pnpm@8.0.0"
}